





a=[1,2,3,4,5]
b=a[1:5:2]
c=a[0:6:2]
print(sum(b))
print(sum(c))


arr =[1,2,3,4,5,6,7]
evenSum = 0
oddSum=0
for i in arr:
   if i%2==0:
    evenSum=evenSum+i
    print(evenSum)
   else:
    oddSum=oddSum+i
    
    print(oddSum)


c=["abcd","efghi","ghifh","dskf","sdddfsdf"]
d=c[0][0]+c[1][0]+c[2][0]+c[3][0]+c[4][0]
print(f'"{d}"')


p="python"
q=p[::-1]
print(q)








